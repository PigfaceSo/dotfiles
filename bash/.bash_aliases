# .bash_aliases

alias vim="nvim"
alias cc="clear"
alias ls="ls --color=auto"
alias ll="ls -alF"
alias grep="grep --color=auto"
alias jq="jq --color-output"
alias t="task"

alias node-redd="nohup node-red -v > ~/node-red.log &"
alias mosquittod="nohup mosquitto -c /etc/mosquitto/mosquitto.conf -v > ~/mosquitto.log &"
alias ac="arduino-cli compile"
alias am="arduino-cli monitor"
alias abd="arduino-cli board details -b"
alias abl="arduino-cli board list"

